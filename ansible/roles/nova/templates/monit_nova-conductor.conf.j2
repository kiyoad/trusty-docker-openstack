check process nova-conductor with pidfile /var/run/nova-conductor.pid
  start program = "/sbin/start-stop-daemon --start --quiet --background --pidfile /var/run/nova-conductor.pid --chuid nova --chdir /var/run --name nova-conductor --exec /usr/bin/nova-conductor -- --config-file=/etc/nova/nova.conf"
  stop program  = "/sbin/start-stop-daemon --stop --quiet --retry=TERM/30/KILL/5 --pidfile /var/run/nova-conductor.pid --name nova-conductor"
